<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Sheridan C. Rawlins</author>
    <sampleQuery>select * from {table}</sampleQuery>
  </meta>
  <bindings>
    <select itemPath="Catalog.Table.TableField" produces="XML">
      <urls>
        <url>http://{storeID}.stores.yahoo.net/catalog.xml</url>
      </urls>
      <inputs>
        <key id="storeID" type="xs:string" paramType="path" required="true"/>
      </inputs>
      <execute>
        <![CDATA[
                 response.object = y.rest(request.url).get().response;
        ]]>
      </execute>
    </select>
  </bindings>
</table>
