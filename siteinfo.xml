<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Sheridan C. Rawlins</author>
    <sampleQuery>select * from {table} where storeID="finevines"</sampleQuery>
  </meta>
  <bindings>
    <select itemPath="SiteInfo" produces="XML">
      <urls>
        <url>http://store.yahoo.com/{storeID}/siteinfo</url>
      </urls>
      <inputs>
        <key id="storeID" type="xs:string" paramType="path" required="true"/>
      </inputs>
      <execute><![CDATA[
      var fetch = request.get().response;
      response.object = <SiteInfo>{fetch}</SiteInfo>;
      ]]></execute>
    </select>
  </bindings>
</table>
